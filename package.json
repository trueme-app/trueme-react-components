    "@types/react-dom": "16.8.4",
    "@types/react-native": "^0.60.0",
    "@types/react-test-renderer": "^16.8.2",
    "@types/styled-components": "^4.1.18",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
